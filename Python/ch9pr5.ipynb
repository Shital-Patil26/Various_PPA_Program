    "words = [\"donkey\",\"kaddu\",\"mote\"]\n",
    "\n",
    "with open(\"sam.txt\") as f:\n",
    "    content = f.read()\n",
    "    \n",
    "for word in words:\n",
    "    content = content.replace(word,\"$%^&**\")\n",
    "    with open(\"sam.txt\",'w') as f:\n",
    "        f.write(content)"
